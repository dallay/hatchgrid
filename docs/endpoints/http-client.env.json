{
	"dev": {
		"url": "http://localhost:8080",
		"keycloak-url": "http://localhost:9080",
		"realm": "hatchgrid",
		"clientId": "web_app",
		"clientSecret": "web_app",
		"Security": {
			"Auth": {
				"auth-id": {
					"Type": "OAuth2",
					"Grant Type": "Authorization Code",
					"Client ID": "web_app",
					"Auth URL": "{{keycloak-url}}/realms/{{realm}}/protocol/openid-connect/auth",
					"Token URL": "{{keycloak-url}}/realms/{{realm}}/protocol/openid-connect/token",
					"Redirect URL": "{{url}}/oauth2/callback",
					"Scope": "openid profile email"
				}
			}
		}
	}
}
